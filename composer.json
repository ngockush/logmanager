{
    "name": "ngockush/logmanager",
    "description": "An interface to preview, download and delete Laravel log files.",
    "keywords": [
        "ngockush",
        "log manager",
        "logs",
        "laravel",
        "log interface"
    ],
    "homepage": "https://github.com/ngockush/logmanager",
    "license": "proprietary",
    "authors": [{
        "name": "Cristian Tabacitu",
        "role": "Developer"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ngockush/crud": "^4.1|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "ngockush\\LogManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ngockush\\LogManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ngockush\\LogManager\\LogManagerServiceProvider"
            ]
        }
    }
}